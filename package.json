{
  "name": "supertokens-website",
  "version": "3.2.2",
  "description": "frontend sdk for website to be used for auth solution.",
  "main": "index.js",
  "dependencies": {
    "browser-tabs-lock": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.5.0",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@babel/preset-env": "7.5.2",
    "@babel/register": "7.4.4",
    "@babel/runtime": "7.5.2",
    "cookie-parser": "1.4.4",
    "express": "4.17.1",
    "isomorphic-fetch": "2.2.1",
    "mocha": "6.1.4",
    "mocha-jsdom": "2.0.0",
    "prettier": "1.18.2",
    "supertokens-node-mysql-ref-jwt": "^4.0.0",
    "typescript": "3.5.2",
    "axios": "*",
    "mysql": "2.17.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "peerDependencies": {
    "axios": "*"
  },
  "scripts": {
    "test": "TEST_MODE=testing npx mocha",
    "build": "npx tsc -p tsconfig.json",
    "build-check": "npx tsc -p tsconfig.json --noEmit",
    "pretty": "npx prettier --write --config .prettierrc \"*.ts\" \"*.js\" \"test/**/*.js\"",
    "pretty-check": "npx prettier --check --config .prettierrc \"*.ts\" \"*.js\" \"test/**/*.js\"",
    "build-pretty": "npm run build && npm run pretty",
    "set-up-hooks": "cp hooks/pre-commit.sh .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "pack": "./node_modules/.bin/webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supertokens/supertokens-website.git"
  },
  "keywords": [
    "auth",
    "authentication",
    "authorisation",
    "supertokens",
    "chrome",
    "firefox",
    "IE",
    "safari"
  ],
